@page
@model RazorPagesMovie.Pages.Movies.UpcomingModel
<h2>Upcoming Movies</h2>

<table class="table">
    <thead>
        <tr>
            <th>Title</th>
            <th>Release Date</th>
        </tr>
    </thead>
<tbody>
    @foreach (var movie in Model.UpcomingMovies)
        {
         <tr>
             <td>@movie.Title</td>
             <td>@movie.ReleaseDate.ToShortDateString</td>
         </tr>    
        }
        </tbody>
        </table>
